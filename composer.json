{
    "name": "christian-schoenefeld/laravel-vue-i18n-generator",
    "license": "MIT",
    "description": "Generates a vue-i18n compatible include file from your Laravel translations.",
    "keywords": ["laravel","vue-i18n"],
    "homepage": "https://github.com/christian-schoenefeld/laravel-vue-i18n-generator",
    "authors": [
        {
            "name": "Martin Lindhe",
            "email": "martin@ubique.se"
        },
        {
            "name": "Christian Sch√∂nefeld",
            "email": "me@christian-schoenefeld.de"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "illuminate/console": "^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "ChristianSchoenefeld\\VueInternationalizationGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ChristianSchoenefeld\\VueInternationalizationGenerator\\GeneratorProvider"
            ]
        }
    }
}
